        },
        {
          "type": "WebColumn",
          "field": "UnitPrice",
          "header": "Unit Price",
          "dataType": "Number"
        },
        {
          "type": "WebColumn",
          "field": "Price",
          "header": "Price",
          "dataType": "Number"
        },
        {
          "type": "WebColumn",
          "field": "OrderDate",
          "header": "Order Date",
          "dataType": "Date"
        },
        {
          "type": "WebColumn",
          "field": "Delivered",
          "header": "Delivered",
          "dataType": "Boolean"
        }
      ]
    }
