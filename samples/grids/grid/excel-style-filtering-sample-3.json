{
  "descriptions": {
    "content": {
      "type": "WebGrid",
      "name": "grid",
      "dataRef": "NwindData",
      "moving": true,
      "autoGenerate": false,
      "allowFiltering": true,
      "filterMode": "ExcelStyleFilter",
      "excelStyleHeaderIconTemplateRef": "FilterAltIconTemplate",
	    "toolbar": [
        {
          "type": "WebGridToolbar"          
        }
      ],
      "columns": [{
        "type": "WebGridColumn",
        "name": "ProductName",
        "field": "ProductName",
        "header": "Product Name"
      },{
        "type": "WebGridColumn",
        "name": "QuantityPerUnit",
        "field": "QuantityPerUnit",
        "header": "Quantity Per Unit" 
      },{
        "type": "WebGridColumn",
        "name": "UnitPrice",
        "field": "UnitPrice",
        "header": "Unit Price",
        "dataType": "Currency",
        "bodyTemplateRef": "UnitPriceCellTemplate"
      },{
        "type": "WebGridColumn",
        "name": "OrderDate",
        "field": "OrderDate",
        "header": "Order Date",
        "dataType": "Date"
      },{
        "type": "WebGridColumn",
        "name": "Discontinued",
        "field": "Discontinued",
        "header": "Discontinued",
        "bodyTemplateRef": "BooleanCellTemplate"
      }]
    }
  },
  "modules": [
    "withDescriptions",
    "layouts/PropertyEditorPanelModule",
    "grids/WebGridModule"  
  ]
}
